{
  "name": "@nestjsplus/config",
  "version": "1.0.15",
  "description": "Flexible, Docker-friendly, Dotenv-based Configuration Module for NestJs",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:debug": "export DEBUG=cfg,trace; jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "nestjs",
    "configuration",
    "config",
    "module",
    "environment",
    "variables",
    "env",
    ".env",
    "settings"
  ],
  "author": "John Biundo <johnfbiundo@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@types/hapi__joi": "^15.0.2",
    "cli-color": "^1.4.0",
    "debug": "^4.1.1",
    "dotenv": "^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/config"
  },
  "bugs": "https://github.com/nestjsplus/config/issues",
  "peerDependencies": {
    "@nestjs/common": "^9.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@nestjs/common": "^9.3.0",
    "@nestjs/core": "^9.3.0",
    "@nestjs/testing": "^9.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.0",
    "typescript": "^4.9.0",
    "wait-on": "^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
